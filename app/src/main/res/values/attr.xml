<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FloatSeekBar">
        <attr name="floatMax" format="float" />
        <attr name="floatMin" format="float" />
    </declare-styleable>
    <declare-styleable name="WaveBar">
        <attr name="wb_color" format="color" />
        <attr name="wb_radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension"/>
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension"/>
        <attr name="indicatorName" format="string"/>
        <attr name="indicatorColor" format="color"/>
    </declare-styleable>

</resources>